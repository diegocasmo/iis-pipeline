<?xml version="1.0" encoding="utf-8"?>
<!-- happiness, surprise, anger, sadness, disgust, fear.-->
<!-- the commented parameters are the missing AUs - Action units - that iristk cant represent -->
<gestures framerate="50" xmlns="iristk.agent.face.gestures" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="iristk.agent.face.gestures gestures.xsd">
	<gesture name="emotion_neutral">
		<param name="EXPR_ANGER">16(0)</param>		
		<param name="EXPR_DISGUST">16(0)</param>
		<param name="EXPR_FEAR">16(0)</param>
		<param name="EXPR_SAD">16(0)</param>
		<param name="BROW_DOWN_RIGHT">16(0) </param>
		<param name="BROW_DOWN_LEFT">16(0)</param>		
		<param name="BROW_IN_RIGHT">16(0) </param>
		<param name="BROW_IN_LEFT">16(0) </param>
		<param name="EYE_SQUINT_LEFT">16(0) </param>
		<param name="EYE_SQUINT_RIGHT">16(0)</param>
		<param name="PHONE_B_M_P">16(0)</param>
		<param name="PHONE_F_V">16(0)</param>
		<param name="SMILE_CLOSED">16(0)</param>
		<param name="EPICANTHIC_FOLD">16(0)</param>
		<param name="BROW_UP_RIGHT">16(0)</param>
		<param name="BROW_UP_LEFT">16(0)</param>
		<param name="PHONE_BIGAAH">16(0)</param>
		<param name="PHONE_D_S_T">16(0)</param>
	</gesture>

	<gesture name="HAPPY">
		<!-- raise the cheeks (AU6) -->
		<param name="EYE_SQUINT_LEFT">16(0.3) 16(0.4)</param>
		<param name="EYE_SQUINT_RIGHT">16(0.3) 16(0.4)</param>
		<!-- raise lip corners (AU12) -->
		<param name="SMILE_CLOSED">16(0.8) 16(0.8)</param>
	</gesture>

	<gesture name="ANGER">
	<!-- Brows Frown AU4 -->
		<param name="BROW_DOWN_RIGHT">16(1.9) 1000(2)</param>
		<param name="BROW_DOWN_LEFT">16(1.9) 1000(2)</param>
		<param name="BROW_IN_RIGHT">16(0.5) 16(0.5)</param>
		<param name="BROW_IN_LEFT">16(0.5) 16(0.5)</param>
	<!-- Lid Tightener AU7 -->
		<!--param name="EPICANTHIC_FOLD">16(0.1) 16(0.1)</param-->
		<param name="EYE_SQUINT_LEFT">16(1.5) 16(1.5)</param>
		<param name="EYE_SQUINT_RIGHT">16(1.5) 16(1.5)</param>
	<!-- <param name="EXPR_ANGER">16(1) 16(1)</param> -->
	<!-- chin raiser AU17-->
	<!-- Lip PressorAU24 -->
		<param name="PHONE_B_M_P">16(0.5) 16(0.5)</param>
		<param name="PHONE_F_V">16(0.7) 16(0.7)</param>
	</gesture>

	<gesture name="DISGUST">
	<!-- nose wrinkler (AU9)-->		
	<!-- Lid Tightener AU7 -->
		<param name="EYE_SQUINT_LEFT">16(0.9) 16(0.9)</param>
		<param name="EYE_SQUINT_RIGHT">16(0.9) 16(0.9)</param>
	<!-- chin raiser AU17-->
	<!--  upper lip raiser (AU10)  PHONE_EE or PHONE_FV? -->
		<param name="PHONE_F_V">16(1.3) 16(1.3)</param>		
	<!-- depression of the lip corners (AU15)-->
	<param name="PHONE_B_M_P">16(0.3) 16(0.3)</param>
	</gesture>

	<gesture name="SADNESS">
	<!--  Brow Lowerer(AU4) and Inner Brow Raiser(AU1)-->
		<param name="BROW_DOWN_RIGHT">16(0.5) 16(0.5) 16(0.5)</param>
		<param name="BROW_DOWN_LEFT">16(0.5) 16(0.5) 16(0.5)</param>		
		<param name="BROW_IN_RIGHT">16(1.6) 16(1.6) 164(1.6)</param>
		<param name="BROW_IN_LEFT">16(1.6) 16(1.6) 164(1.6)</param>
	<!-- depression of the lip corners (AU15)-->
		<param name="PHONE_B_M_P">16(0.8) 16(0.8) 16(0.8)</param>
		<param name="PHONE_F_V">16(0.8) 16(0.8) 16(0.8)</param>
	</gesture>

	<gesture name="SURPRISE"> 
	<!-- Inner Brow Raiser(AU1)-->
		<param name="BROW_IN_RIGHT">16(0) 16(0.5) 32(0.5)</param>
		<param name="BROW_IN_LEFT">16(0) 16(0.5) 32(0.5)</param>
	<!-- Outer Brow Raiser(AU2)-->
		<param name="BROW_UP_RIGHT">16(0) 16(1.5) 32(1.5)</param>
		<param name="BROW_UP_LEFT">16(0) 16(1.5) 32(1.5)</param>
	<!--  raise upper eyelid (AU5)-->
		<param name="EPICANTHIC_FOLD">16(0.02) 16(0) 32(0)</param>
		<param name="EYE_SQUINT_LEFT">16(0.5) 16(0) 32(0)</param>
		<param name="EYE_SQUINT_RIGHT">16(0.5) 16(0) 32(0)</param>
	<!-- drop the jaw (AU26)-->
		<param name="PHONE_BIGAAH">16(0) 16(0.3) 32(0.3)</param>
	</gesture>
	<gesture name="FEAR">  
	<!-- Inner Brow Raiser(AU1)-->
		<param name="BROW_IN_RIGHT">16(0) 16(1.5) 32(1.5)</param>
		<param name="BROW_IN_LEFT">16(0) 16(1.5) 32(1.5)</param>
	<!--  Brow Lowerer(AU4)-->
		<param name="BROW_DOWN_RIGHT">16(0) 16(1.5) 32(1.5)</param>
		<param name="BROW_DOWN_LEFT">16(0) 16(1.5) 32(1.5)</param>		
		<param name="BROW_IN_RIGHT">16(0) 16(1.5) 32(1.5)</param>
		<param name="BROW_IN_LEFT">16(0) 16(1.5) 32(1.5)</param>
	<!-- raising outer brow(AU2)-->
		<param name="BROW_UP_RIGHT">16(0) 16(1.5) 32(1.5)</param>
		<param name="BROW_UP_LEFT">16(0) 16(1.5) 32(1.5)</param>
	<!-- raise upper eyelid (AU5-->
		<param name="EPICANTHIC_FOLD">16(0.02) 16(0) 32(0)</param>
		<param name="EYE_SQUINT_LEFT">16(0.5) 16(0) 32(0)</param>
		<param name="EYE_SQUINT_RIGHT">16(0.5) 16(0) 32(0)</param>
	<!--  drop the jaw (AU26) N EH BIGAAH -->
		<param name="PHONE_BIGAAH">16(0) 16(0.2) 32(0.2)</param>
	<!--  stretch lips (AU20)-->
		<param name="PHONE_D_S_T">16(0) 16(0.1) 32(0.1)</param>
	<!-- depression of the lip corners (AU15)-->
	</gesture>

	<gesture name="nod">
		<param name="NECK_TILT">16(10) 16(0)</param>
	</gesture>
	<gesture name="shake">
		<param name="NECK_PAN">16(10) 32(-10) 16(0)</param>
	</gesture>
	<gesture name="smile">
		<param name="SMILE_CLOSED">16(0.5) 16(0.5) 16(0)</param>
		<param name="BROW_UP_LEFT">16(1) 16(1) 16(0)</param>		
		<param name="BROW_UP_RIGHT">16(1) 16(1) 16(0)</param>
	</gesture>
	<gesture name="brow_up">
		<param name="BROW_UP_LEFT">16(1)</param>		
		<param name="BROW_UP_RIGHT">16(1)</param>
	</gesture>
	<gesture name="brow_down">
		<param name="BROW_UP_LEFT">16(0)</param>		
		<param name="BROW_UP_RIGHT">16(0)</param>
	</gesture>
	<gesture name="brow_raise">
		<param name="BROW_UP_LEFT">16(1) 16(1) 16(0)</param>		
		<param name="BROW_UP_RIGHT">16(1) 16(1) 16(0)</param>
	</gesture>
	<gesture name="brow_frown">
		<param name="BROW_DOWN_RIGHT">16(1) 16(1) 16(0)</param>
		<param name="BROW_DOWN_LEFT">16(1) 16(1) 16(0)</param>		
		<param name="BROW_IN_RIGHT">16(1) 16(1) 16(0)</param>
		<param name="BROW_IN_LEFT">16(1) 16(1) 16(0)</param>
		<param name="EYE_SQUINT_LEFT">16(0.6) 16(0.6) 16(0)</param>
		<param name="EYE_SQUINT_RIGHT">16(0.6) 16(0.6) 16(0)</param>
	</gesture>
	<gesture name="sleep">
		<param name="BLINK_LEFT">40(1)</param>
		<param name="BLINK_RIGHT">40(1)</param>
	</gesture>
	<gesture name="wake_up">
		<param name="BLINK_LEFT">40(0)</param>
		<param name="BLINK_RIGHT">40(0)</param>
	</gesture>
	<gesture name="blink">
		<param name="BLINK_LEFT">8(1) 8(0)</param>
		<param name="BLINK_RIGHT">8(1) 8(0)</param>
	</gesture>
	<gesture name="gaze_away">
		<param name="GAZE_PAN">16(20) 32(20) 16(0)</param>
		<param name="GAZE_TILT">16(20) 32(20) 16(0)</param>
	</gesture>
	<gesture name="express_disgust">
		<param name="EXPR_ANGER">16(0)</param>		
		<param name="EXPR_DISGUST">16(1) 32(1) 16(0)</param>
		<param name="EXPR_FEAR">16(0)</param>
		<param name="EXPR_SAD">16(0)</param>
	</gesture>
	<gesture name="express_sad">
		<param name="EXPR_ANGER">16(0)</param>		
		<param name="EXPR_DISGUST">16(0)</param>
		<param name="EXPR_FEAR">16(0)</param>
		<param name="EXPR_SAD">16(1) 32(1) 16(0)</param>
	</gesture>
	<gesture name="emotion_anger">
		<param name="EXPR_ANGER">16(1)</param>		
		<param name="EXPR_DISGUST">16(0)</param>
		<param name="EXPR_FEAR">16(0)</param>
		<param name="EXPR_SAD">16(0)</param>
	</gesture>
	<gesture name="emotion_disgust">
		<param name="EXPR_ANGER">16(0)</param>		
		<param name="EXPR_DISGUST">16(1)</param>
		<param name="EXPR_FEAR">16(0)</param>
		<param name="EXPR_SAD">16(0)</param>
	</gesture>
	<gesture name="emotion_fear">
		<param name="EXPR_ANGER">16(0)</param>		
		<param name="EXPR_DISGUST">16(0)</param>
		<param name="EXPR_FEAR">16(1)</param>
		<param name="EXPR_SAD">16(0)</param>
	</gesture>
	<gesture name="emotion_sad">
		<param name="EXPR_ANGER">16(0)</param>		
		<param name="EXPR_DISGUST">16(0)</param>
		<param name="EXPR_FEAR">16(0)</param>
		<param name="EXPR_SAD">16(1)</param>
	</gesture>
	
	<!-- Gestures used in Birthday Skill -->
	<gesture name="smile_on">
		<param name="SMILE_CLOSED">16(1.5)</param>
		<param name="BROW_UP_LEFT">16(1) 16(1) 16(1) 16(1) 16(0)</param>		
	</gesture>
	<gesture name="smile_off">
		<param name="SMILE_CLOSED">16(0)</param>		
	</gesture>
	<gesture name="laugh">
		<param name="SMILE_OPEN">16(1.0) 16(0) 16(1.4) 32(0)</param>
		<param name="BROW_UP_LEFT">16(1) 16(1) 16(1)  16(0)</param>		
		<param name="BROW_UP_RIGHT">16(1) 16(1) 16(1) 16(0)</param>		
		<param name="EYE_SQUINT_LEFT">16(1) 16(0) 16(0.4) 16(0) 16(0)</param>		
		<param name="EYE_SQUINT_RIGHT">16(1) 16(0) 16(0.4) 16(0) 16(0)</param>		
		<param name="PHONE_F_V">16(1) 16(0) 16(0.4) 16(0) 16(0)</param>		
		<param name="PHONE_TH">16(1) 16(0) 16(0.4) 16(0) 16(0)</param>		
	</gesture>
	<!-- End of gestures used in Birthday Skill -->
</gestures>

 